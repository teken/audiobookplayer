{
  "name": "audiobookplayer",
  "version": "0.1.17",
  "description": "A new audiobook player",
  "author": "Anna Haig",
  "private": true,
  "dependencies": {
    "@babel/core": "7.9.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "camelcase": "^5.3.1",
    "configstore": "^5.0.1",
    "deep-equal": "^1.0.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "electron-updater": "^6.6.2",
    "fs-extra": "^8.1.0",
    "fuse.js": "3.4.6",
    "identity-obj-proxy": "3.0.0",
    "lokijs": "1.5.12",
    "music-metadata": "11.3.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-id-generator": "3.0.0",
    "react-lazy-load": "^3.1.13",
    "react-router-dom": "^5.2.0",
    "react-table": "7.5.1",
    "resolve": "1.15.0",
    "semver": "6.3.0",
    "ts-pnp": "1.1.6",
    "uuid": "^11.1.0"
  },
  "main": "electron/electron.cjs",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:electron": "electron .",
    "start:electron:dev": "ELECTRON_START_URL=http://localhost:5173 NODE_ENV=development electron .",
    "pack": "pnpm build && electron-builder --dir",
    "dist": "pnpm build && electron-builder",
    "dist:linux": "pnpm build && electron-builder --linux",
    "dist:linux:snap": "pnpm build && electron-builder --linux snap",
    "dist:mac": "pnpm build && electron-builder --macos",
    "dist:win": "pnpm build && electron-builder --win",
    "release": "electron-builder --publish always",
    "release:linux": "electron-builder --linux --publish always",
    "release:mac": "electron-builder --macos --publish always",
    "release:win": "electron-builder --win --publish always",
    "eb-start": "electron-builder start",
    "eb-create-certs": "electron-builder create-self-signed-cert",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.5.2",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "vite": "^6.3.5"
  },
  "build": {
    "appId": "app.AudioBookPlayer",
    "productName": "Audio Book Player",
    "copyright": "Copyright Â© 2018 - 2025 Anna Haig",
    "files": [
      "build/**/*",
      "src/services/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "icon": "public/icon.ico",
      "publish": {
        "provider": "s3",
        "bucket": "audiobookplayer",
        "encryption": "AES256",
        "acl": "public-read",
        "path": "/builds/win/"
      }
    },
    "linux": {
      "target": "AppImage",
      "category": "Audio",
      "publish": {
        "provider": "s3",
        "bucket": "audiobookplayer",
        "encryption": "AES256",
        "acl": "public-read",
        "path": "/builds/linux/"
      }
    },
    "mac": {
      "category": "public.app-category.music",
      "icon": "public/icon.icns",
      "publish": {
        "provider": "s3",
        "bucket": "audiobookplayer",
        "encryption": "AES256",
        "acl": "public-read",
        "path": "/builds/mac/"
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "/home/anna/dev/audiobookplayer/node_modules/.pnpm/node_modules"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
